        <string>A8B61C</string>
      </PointerList>
      <Active>true</Active>
    </Watch>
  </watchList>
  <triggerList>
    <Trigger>
      <id>1</id>
      <name>Clock trigger for InGame vs InMenu determination</name>
      <description>Round timer. 90 down to 0 as an int</description>
      <value xsi:type="xsd:string">90</value>
      <triggerType>Normal</triggerType>
      <allowanceType>InGame</allowanceType>
      <watchOneId>1</watchOneId>
      <secondaryId>-1</secondaryId>
      <comparisonType>EqualTo</comparisonType>
      <sampleFilename>NONE</sampleFilename>
      <sampleSpeed>1</sampleSpeed>
      <sampleVolume>1</sampleVolume>
      <useTolk>false</useTolk>
      <isClock>true</isClock>
      <active>true</active>
    </Trigger>
    <Trigger>
      <id>2</id>
      <name>P1 Health Half</name>
