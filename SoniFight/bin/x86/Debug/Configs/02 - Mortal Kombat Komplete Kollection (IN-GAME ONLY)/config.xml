<?xml version="1.0" encoding="utf-8"?>
<GameConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <watchList>
    <Watch>
      <valueType>IntType</valueType>
      <Id>1</Id>
      <Name>Clock</Name>
      <Description>Clock as int. Range is 90 down to 0.</Description>
      <PointerList>
        <string>A91C80</string>
      </PointerList>
      <Active>true</Active>
    </Watch>
    <Watch>
      <valueType>FloatType</valueType>
      <Id>2</Id>
      <Name>P1 Health</Name>
      <Description>Player 1 health as a float. Range is 1.0f to 0.0f</Description>
      <PointerList>
        <string>AD8970</string>
        <string>180</string>
        <string>9C</string>
        <string>130</string>
        <string>284</string>
      </PointerList>
      <Active>true</Active>
    </Watch>
    <Watch>
      <valueType>FloatType</valueType>
      <Id>3</Id>
      <Name>P1 X-Ray Bar</Name>
      <Description>The X-Ray bar for player 1 as a float. Range 0.0f to 1.0f.</Description>
      <PointerList>
        <string>A8A990</string>
      </PointerList>
      <Active>true</Active>
    </Watch>
    <Watch>
      <valueType>FloatType</valueType>
      <Id>4</Id>
      <Name>P2 Health</Name>
      <Description>Player 2 health as a float. Range 1.0f to 0.0f.</Description>
