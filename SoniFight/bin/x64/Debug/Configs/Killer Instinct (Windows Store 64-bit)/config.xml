<?xml version="1.0" encoding="utf-8"?>
<GameConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <GameConfigFileVersion>1</GameConfigFileVersion>
  <watchList>
    <Watch>
      <valueType>IntType</valueType>
      <Id>1</Id>
      <Name>Clock</Name>
      <Description>Round timer from 99 down to 0 as int</Description>
      <PointerList>
        <string>31DE8E8</string>
        <string>368</string>
      </PointerList>
      <Active>true</Active>
    </Watch>
    <Watch>
      <valueType>IntType</valueType>
      <Id>2</Id>
      <Name>Main Menu options</Name>
      <Description>Integer, range 0 through 4.</Description>
      <PointerList>
        <string>31EA360</string>
        <string>358</string>
        <string>640</string>
        <string>38</string>
        <string>150</string>
        <string>B0</string>
      </PointerList>
      <Active>true</Active>
    </Watch>
  </watchList>
  <triggerList>
    <Trigger>
      <triggerType>Normal</triggerType>
      <allowanceType>Any</allowanceType>
      <comparisonType>EqualTo</comparisonType>
      <Id>1</Id>
      <Name>Clock</Name>
      <Description>Clock watch for InGame and InMenu determination</Description>
      <Value xsi:type="xsd:int">-1</Value>
      <WatchIdList>
        <int>1</int>
      </WatchIdList>
      <SecondaryIdList>
        <int>-1</int>
      </SecondaryIdList>
      <SampleFilename>None</SampleFilename>
      <SampleSpeed>1</SampleSpeed>
      <SampleVolume>1</SampleVolume>
      <UseTolk>false</UseTolk>
      <IsClock>true</IsClock>
      <Active>true</Active>
    </Trigger>
    <Trigger>
      <triggerType>Normal</triggerType>
      <allowanceType>InGame</allowanceType>
      <comparisonType>EqualTo</comparisonType>
      <Id>2</Id>
      <Name>Clock 50</Name>
      <Description>50</Description>
      <Value xsi:type="xsd:string">50</Value>
      <WatchIdList>
        <int>1</int>
      </WatchIdList>
      <SecondaryIdList>
        <int>-1</int>
      </SecondaryIdList>
      <SampleFilename>50</SampleFilename>
      <SampleSpeed>1</SampleSpeed>
      <SampleVolume>1</SampleVolume>
      <UseTolk>true</UseTolk>
      <IsClock>false</IsClock>
      <Active>true</Active>
    </Trigger>
    <Trigger>
      <triggerType>Normal</triggerType>
      <allowanceType>InGame</allowanceType>
      <comparisonType>EqualTo</comparisonType>
      <Id>3</Id>
